<section class="pagination">
  <a href="?page=<%= firstPage %>">
    First
  </a>
  <% if (hasPreviousPage) { %>
    <a href="?page=<%= previousPage %>">
      <%= previousPage %>
    </a>
    <% } %>
      <a class="active" href="?page=<%= currentPage %>">
        <%= currentPage %>
      </a>
      <% if (hasNextPage) { %>
        <a href="?page=<%= nextPage %>">
          <%= nextPage %>
        </a>
        <% } %>
          <a href="?page=<%= lastPage %>">
            Last
          </a>
</section>